<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ar.com.finit.core.model.impl.PlayerCapacityImpl" table="PLAYER_CAPACITY">
    
       <id name="id" type="java.lang.String">
			<column name="ID" />
			<generator class="ar.com.finit.core.generator.UUIDGenerator" />
		</id>
        
        <property name="text" type="java.lang.String">
            <column name="TEXT" />
        </property>
        
        <many-to-one name="sport" column="SPORT" class="ar.com.finit.core.model.impl.SportImpl" cascade="none" lazy="false" />


    </class>
</hibernate-mapping>
