package ar.com.finit.core.model.impl;

import java.util.List;
import java.util.Set;

import ar.com.finit.core.model.Field;
import ar.com.finit.core.model.Ground;
import ar.com.finit.core.model.Image;
import ar.com.finit.core.model.Irregular;
import ar.com.finit.core.model.PlayerCapacity;
import ar.com.finit.core.model.Sport;
import ar.com.finit.core.model.Tariff;

/**
 * @author leo
 */
public class FieldImpl implements Field {

	private String id;

	private String width;

	private String height;

	private Set<PlayerCapacity> playersCapacity;

	private Set<Image> images;

	private Set<Sport> sports;

	private Tariff tariff;

	private List<Irregular> irregulars;

	private Ground ground;

	private boolean ceiling;

	@Override
	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	@Override
	public String getWidth() {
		return width;
	}

	public void setWidth(String width) {
		this.width = width;
	}

	@Override
	public String getHeight() {
		return height;
	}

	public void setHeight(String height) {
		this.height = height;
	}

	@Override
	public Set<PlayerCapacity> getPlayersCapacity() {
		return playersCapacity;
	}

	public void setPlayersCapacity(Set<PlayerCapacity> playersCapacity) {
		this.playersCapacity = playersCapacity;
	}

	@Override
	public Set<Image> getImages() {
		return images;
	}

	public void setImages(Set<Image> images) {
		this.images = images;
	}

	@Override
	public Set<Sport> getSports() {
		return sports;
	}

	public void setSports(Set<Sport> sports) {
		this.sports = sports;
	}

	@Override
	public Tariff getTariff() {
		return tariff;
	}

	public void setTariff(Tariff tariff) {
		this.tariff = tariff;
	}

	@Override
	public List<Irregular> getIrregulars() {
		return irregulars;
	}

	public void setIrregulars(List<Irregular> irregulars) {
		this.irregulars = irregulars;
	}

	@Override
	public Ground getGround() {
		return ground;
	}

	public void setGround(Ground ground) {
		this.ground = ground;
	}

	@Override
	public boolean isCeiling() {
		return ceiling;
	}

	public void setCeiling(boolean ceiling) {
		this.ceiling = ceiling;
	}

}
