package ar.com.finit.dto.model;

import java.util.List;
import java.util.Set;

/**
 * @author leo
 */
public class FieldDTO {

	private String id;

	private String width;

	private String height;

	private Set<PlayerCapacityDTO> playersCapacity;

	private Set<ImageDTO> images;

	private Set<SportDTO> sports;

	private TariffDTO tariff;

	private List<IrregularDTO> irregulars;

	private GroundDTO ground;

	private boolean ceiling;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getWidth() {
		return width;
	}

	public void setWidth(String width) {
		this.width = width;
	}

	public String getHeight() {
		return height;
	}

	public void setHeight(String height) {
		this.height = height;
	}

	public Set<PlayerCapacityDTO> getPlayersCapacity() {
		return playersCapacity;
	}

	public void setPlayersCapacity(Set<PlayerCapacityDTO> playersCapacity) {
		this.playersCapacity = playersCapacity;
	}

	public Set<ImageDTO> getImages() {
		return images;
	}

	public void setImages(Set<ImageDTO> images) {
		this.images = images;
	}

	public Set<SportDTO> getSports() {
		return sports;
	}

	public void setSports(Set<SportDTO> sports) {
		this.sports = sports;
	}

	public TariffDTO getTariff() {
		return tariff;
	}

	public void setTariff(TariffDTO tariff) {
		this.tariff = tariff;
	}

	public List<IrregularDTO> getIrregulars() {
		return irregulars;
	}

	public void setIrregulars(List<IrregularDTO> irregulars) {
		this.irregulars = irregulars;
	}

	public GroundDTO getGround() {
		return ground;
	}

	public void setGround(GroundDTO ground) {
		this.ground = ground;
	}

	public boolean isCeiling() {
		return ceiling;
	}

	public void setCeiling(boolean ceiling) {
		this.ceiling = ceiling;
	}

}
