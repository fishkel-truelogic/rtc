package ar.com.finit.dto.model.factory;

import java.lang.reflect.Type;
import java.util.Collection;

import ar.com.finit.dto.factory.AbstractDTOFactory;
import ar.com.finit.dto.model.IrregularDTO;

import com.google.gson.reflect.TypeToken;

/**
 * @author leo
 */
public class IrregularDTOFactory extends AbstractDTOFactory<IrregularDTO> {

	@Override
	protected Type getEntityType() {
		return new TypeToken<IrregularDTO>(){}.getType();
	}

	@Override
	protected Type getCollectionType() {
		return new TypeToken<Collection<IrregularDTO>>(){}.getType();
	}
}