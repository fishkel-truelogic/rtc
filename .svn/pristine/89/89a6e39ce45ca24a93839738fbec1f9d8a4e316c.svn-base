<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ar.com.finit.core.model.impl.MapMarkerImpl" table="MAP_MARKER">
    
        <id name="id" type="java.lang.String">
			<column name="ID" />
			<generator class="ar.com.finit.core.generator.UUIDGenerator" />
		</id>
		
		<property name="entity" type="java.lang.String">
            <column name="ENTITY" />
        </property>
        
		<property name="lat" type="java.lang.Double">
            <column name="LAT" />
        </property>
        
		<property name="lng" type="java.lang.Double">
            <column name="LNG" />
        </property>
	
    </class>
</hibernate-mapping>
