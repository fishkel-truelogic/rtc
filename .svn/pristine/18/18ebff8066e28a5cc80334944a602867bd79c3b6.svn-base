package ar.com.finit.dto.model.factory;

import java.lang.reflect.Type;
import java.util.Collection;

import ar.com.finit.dto.factory.AbstractDTOFactory;
import ar.com.finit.dto.model.TariffDTO;

import com.google.gson.reflect.TypeToken;

/**
 * @author leo
 */
public class TariffDTOFactory extends AbstractDTOFactory<TariffDTO> {

	@Override
	protected Type getEntityType() {
		return new TypeToken<TariffDTO>(){}.getType();
	}

	@Override
	protected Type getCollectionType() {
		return new TypeToken<Collection<TariffDTO>>(){}.getType();
	}
}