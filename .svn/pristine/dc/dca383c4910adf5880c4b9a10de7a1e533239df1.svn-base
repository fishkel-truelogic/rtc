<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       					http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

<!-- violation converter	 -->
	<bean id="imageViolationConverter" class="ar.com.finit.service.model.converter.violation.ImageViolationConverter" />
	<bean id="districtViolationConverter" class="ar.com.finit.service.model.converter.violation.DistrictViolationConverter" />
	<bean id="groundViolationConverter" class="ar.com.finit.service.model.converter.violation.GroundViolationConverter" />
	<bean id="sportViolationConverter" class="ar.com.finit.service.model.converter.violation.SportViolationConverter" />
	<bean id="userViolationConverter" class="ar.com.finit.service.model.converter.violation.UserViolationConverter" />
	<bean id="roleViolationConverter" class="ar.com.finit.service.model.converter.violation.RoleViolationConverter" />
	<bean id="playerViolationConverter" class="ar.com.finit.service.model.converter.violation.PlayerViolationConverter" />

<!-- DTO / Entity converter -->
	<bean id="districtConverter" class="ar.com.finit.service.model.converter.DistrictConverter"></bean>
	<bean id="groundConverter" class="ar.com.finit.service.model.converter.GroundConverter"></bean>
	<bean id="sportConverter" class="ar.com.finit.service.model.converter.SportConverter"></bean>
	<bean id="imageConverter" class="ar.com.finit.service.model.converter.ImageConverter"></bean>
	<bean id="userConverter" class="ar.com.finit.service.model.converter.UserConverter"></bean>
	<bean id="roleConverter" class="ar.com.finit.service.model.converter.RoleConverter"></bean>
	<bean id="playerConverter" class="ar.com.finit.service.model.converter.PlayerConverter"></bean>
</beans>