package ar.com.finit.dto.model;

import java.util.List;

/**
 * @author leo
 */
public class BookDTO {

	private String id;

	private String hour;

	private String day;

	private PlayerDTO player;

	private List<PlayerDTO> waitingPlayers;

	private String fieldId;

	private String userId;

	private String stablishmentId;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getHour() {
		return hour;
	}

	public void setHour(String hour) {
		this.hour = hour;
	}

	public String getDay() {
		return day;
	}

	public void setDay(String day) {
		this.day = day;
	}

	public PlayerDTO getPlayer() {
		return player;
	}

	public void setPlayer(PlayerDTO player) {
		this.player = player;
	}

	public List<PlayerDTO> getWaitingPlayers() {
		return waitingPlayers;
	}

	public void setWaitingPlayers(List<PlayerDTO> waitingPlayers) {
		this.waitingPlayers = waitingPlayers;
	}

	public String getFieldId() {
		return fieldId;
	}

	public void setFieldId(String fieldId) {
		this.fieldId = fieldId;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public String getStablishmentId() {
		return stablishmentId;
	}

	public void setStablishmentId(String stablishmentId) {
		this.stablishmentId = stablishmentId;
	}

}
